                            "/home/ubuntu/f5lab-dns-masq.sh": {
                                "content": {
                                    "Fn::Join": [
                                        "",
                                        [
                                            "#!/bin/bash\n",
                                            "# Install dnsmasq later - need these entries in /etc/dnsmasq.d/f5lab\n",
                                            "mkdir -p /etc/dnsmasq.d\n",
                                            "touch /etc/dnsmasq.d/f5lab\n",
                                            "echo \"listen-address=127.0.0.1,",
                                            {
                                                "Ref": "Server1Eth0PrivateIP"
                                            },
                                            ",",
                                            {
                                                "Ref": "Server1Eth01PrivateIP"
                                            },
                                            ",",
                                            {
                                                "Ref": "Server1Eth02PrivateIP"
                                            },
                                            ",",
                                            {
                                                "Ref": "Server1Eth03PrivateIP"
                                            },
                                            ",",
											{
                                                "Ref": "Server1Eth04PrivateIP"
                                            },
                                            ",",											
                                            {
                                                "Ref": "Server1Eth1PrivateIP"
                                            },
                                            ",",
                                            {
                                                "Ref": "Server1Eth11PrivateIP"
                                            },
                                            "\" >> /etc/dnsmasq.d/f5lab",
                                            "\n",
                                            "echo \"no-dhcp-interface=lo0,eth0,eth0:1,eth0:2,eth0:3,eth0:4,eth1,eth1:1\" >> /etc/dnsmasq.d/f5lab",
                                            "\n"
                                        ]
                                    ]
                                },
