        "VPC": {
            "Description": "Existing VPC to use",
            "Type": "AWS::EC2::VPC::Id"
        },
        "Server1Password": {
            "Default": "supernetops",
            "Description": "ubuntu User password for Server1 RDP access",
            "Type": "String"
        },
        "InstanceType": {
            "AllowedValues": [
                "t2.medium",
                "t2.large",
                "m3.medium",
                "m3.large"
