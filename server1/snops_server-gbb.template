                                "#!/bin/bash -xe\n",
                                "touch /home/ubuntu/alert1-of-4-started-cloudinit-userdata\n",
                                "# Install the files and packages from the metadata\n",
                                "# ensure static eth0 is working so we can get cfn-init and dont error out too early\n",
                                "ifconfig eth0 ",
                                {
                                    "Ref": "Server1Eth0PrivateIP"
                                },
                                " netmask 255.255.255.0\n",
                                "apt-get -y update\n",
                                "apt-get -y install python-setuptools\n",
                                "easy_install https://s3.amazonaws.com/cloudformation-examples/aws-cfn-bootstrap-latest.tar.gz\n",
                                "cfn-init -v ",
                                "         --stack ",
                                {
                                    "Ref": "AWS::StackName"
                                },
                                "         --resource Server1Instance ",
                                "         --region ",
                                {
                                    "Ref": "AWS::Region"
                                },
                                "\n"
                            ]
